{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "installCommand": "npm install",
    "builds": [
        {
            "src": "build/server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "build/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "build/server.js"
        }
    ]
}